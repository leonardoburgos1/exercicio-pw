var input = require('readline-sync');

var n1  = input.questionInt("digite um numero:");
var n2 = input.questionInt("digite um numero:");
var n3 = input.questionInt("digite um numero:");

let num1 = n1 + 5;
let num2 = n2 + 5;
let num3 = n3 + 5;

if( num1, num2, num3 > 10){
    console.log(" passou no teste")
}